package com.leetcode;

import java.util.Arrays;
import java.util.List;

public class Solution {
	public static void main(String[] args) {
		String str=  "a";
		//int ans = length(str);
		int ans = length(str);
		System.out.println(ans);
		
	
	}
	public static int length(String str) {
		

		String strNew= "";
		
		
		strNew = str.stripTrailing();
		
		String tofind;
		
		
		int ans = 1;
		int count = 0;
		for(int i = strNew.length()-1;i>=0;i--) {
			char ch = strNew.charAt(i);
			tofind = String.valueOf(ch);
			if(tofind.equals(" ")) {
				
				return count;
				
			}
			else  {
				count++;
			}
		}
	
			
		
		return count;
	}

	
	 


}
